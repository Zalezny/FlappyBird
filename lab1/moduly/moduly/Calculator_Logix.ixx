export module Calculator_Logix;

#include<iostream>
#include<cmath>
#include<Windows.h>


export double add(double x, double y) {
	return x + y;
}
export double remove(double x, double y) {
	return x - y;
}
export double multi(double x, double y) {
	return x * y;
}
export double diversion(double x, double y) {
	return x / y;
}

